    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-sharp": "^2.3.7",
    "intersection-observer": "^0.7.0",
    "lodash.debounce": "^4.0.8",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hover-animation": "^2.1.1",
    "react-scrollbars-custom": "^4.0.21",
    "react-typography": "^0.16.19",
    "styled-components": "^5.0.0",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "axe-core": "^3.5.3",
    "cypress": "4.6.0",
    "cypress-axe": "^0.8.1",
    "gatsby-cypress": "^0.4.1",
    "lodash.random": "^3.2.0",
    "netlify-plugin-cypress": "^1.3.9",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.11.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 ",
    "cleandevelop": "gatsby clean && gatsby develop  -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "cleanstart": "yarn cleandevelop",
    "serve": "gatsby serve -p 8000",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:dev:run": "CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:dev:open": "CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:open",
    "test:build:open": "CYPRESS_SUPPORT=y yarn build && start-server-and-test serve http://localhost:8000 cy:open",
    "test:build:run": "CYPRESS_SUPPORT=y yarn build && start-server-and-test serve http://localhost:8000 cy:run"
  }
}
